{
  "name": "@ourpeter/wayke-ecom-web-test",
  "version": "1.1.0",
  "description": "",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node ./esbuild/start.js",
    "build": "npm run clean && node ./esbuild/lib.js && node ./esbuild/cdn.js",
    "build:dev": "node ./esbuild/dev.js",
    "build:types": "tsc --project tsconfig.build.json --outFile dist/types --emitDeclarationOnly --declaration",
    "clean": "rimraf build && rimraf dist && rimraf dist-cdn",
    "test": "npm run lint && npm run typecheck",
    "typecheck": "tsc -noEmit",
    "typecheck:watch": "tsc -noEmit -watch",
    "lint": "eslint --ext .ts,.js 'src'",
    "lint:fix": "eslint --ext .ts,.js 'src' --fix",
    "stylelint": "stylelint '**/*.scss'",
    "prepare": "husky install"
  },
  "author": "wayke",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@types/redux-watch": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "esbuild": "^0.14.10",
    "esbuild-sass-plugin": "^2.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "npm-dts": "^1.3.11",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@wayke-se/ecom": "^3.5.2",
    "redux": "^4.1.2",
    "redux-watch": "^1.2.0"
  },
  "files": [
    "dist"
  ]
}
