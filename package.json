{
  "name": "@wayke-se/ecom-web",
  "version": "0.0.8",
  "description": "Wayke Ecom Web",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node ./esbuild/start.js",
    "cl": "node ./esbuild/cl.js",
    "build": "npm run clean && node ./esbuild/lib.js && node ./esbuild/cdn.js",
    "clean": "rimraf build && rimraf dist && rimraf dist-cdn",
    "test": "npm run lint && npm run typecheck",
    "typecheck": "tsc -noEmit",
    "typecheck:watch": "tsc -noEmit -watch",
    "lint": "eslint --ext .ts,.js 'src'",
    "lint:fix": "eslint --ext .ts,.js 'src' --fix",
    "stylelint": "stylelint '**/*.scss'",
    "prepare": "husky install"
  },
  "author": "Wayke",
  "license": "ISC",
  "devDependencies": {
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.18",
    "@types/object-path": "^0.11.1",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "esbuild": "^0.14.22",
    "esbuild-sass-plugin": "^2.2.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "npm-dts": "^1.3.11",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@wayke-se/ecom": "^3.7.0-alpha.10",
    "marked": "^4.0.16",
    "object-path": "^0.11.8",
    "redux": "^4.1.2",
    "sanitize-html": "^2.7.0"
  },
  "files": [
    "dist"
  ]
}
